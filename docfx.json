{
  "build": {
    "template": [
      "default"
    ],
    "content": [
      {
        "files": [ "index.md", "toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [ "logo.png", "favicon.ico" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "docker docfx",
      "_appFooter": "<span>Generated by <strong>DockerDocFX</strong></span>",
      "_appLogoPath": "logo.png",
      "_appFaviconPath": "favicon.ico",
      "_disableNavbar": false,
      "_disableBreadcrumb": true,
      "_disableToc": false,
      "_disableAffix": false,
      "_disableContribution": true,
      "_enableSearch": false,
      "_enableNewTab": false
    },
    "dest": "wwwroot"
  }
}
